    "version": "0.2.0",
    "configurations": [
        {
            "name": "iss_pointer",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/iss_pointer",
            "args": [
                /*Breite=*/ "-48.1491",
                /*Laenge=*/ "21.4662"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
